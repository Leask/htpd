{
    "name": "websrv",
    "description": "A straightforward web framework build for common tasks.",
    "version": "1989.7.36",
    "private": false,
    "homepage": "https://github.com/Leask/websrv",
    "main": "index.mjs",
    "type": "module",
    "engines": {
        "node": ">=16.x"
    },
    "scripts": {
        "start": "node index.mjs",
        "debug": "node --inspect --trace-warnings debug.mjs",
        "test": "node --inspect --trace-warnings test.mjs",
        "updep": "npx ncu -u && npm install",
        "gitsync": "( git commit -am \"Released @ `date`\" || true ) && git pull && git push",
        "build": "npm run updep && ( git commit -am 'update dependencies' || true ) && npm version patch",
        "prepublishOnly": "npm run build && npm run gitsync",
        "beta": "npm publish --tag beta"
    },
    "author": "Leask Wong <i@leaskh.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Leask/websrv"
    },
    "dependencies": {
        "@google-cloud/storage": "^5.18.2",
        "@koa/cors": "^3.2.0",
        "@koa/multer": "^3.0.0",
        "@koa/router": "^10.1.1",
        "@sentry/tracing": "^6.19.1",
        "domain": "^0.0.1",
        "http-status": "^1.5.0",
        "koa": "^2.13.4",
        "koa-bodyparser": "^4.3.0",
        "koa-json": "^2.0.2",
        "koa-logger": "^3.2.1",
        "koa-passport": "^4.1.4",
        "koa-session": "^6.2.0",
        "koa-static": "^5.0.0",
        "koa-useragent": "^4.0.0",
        "multer": "^1.4.4",
        "passport-twitter": "^1.0.4",
        "utilitas": "^1989.10.18"
    }
}
